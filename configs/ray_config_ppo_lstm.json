{
  "resources": {
    "num_gpus_master": 0,
    "num_cpus_master": 1,
    "num_cpus_per_worker": 0,
    "num_gpus_per_worker": 0,
    "num_learner_workers": 0,
    "num_gpus_per_learner_worker": 0,
    "num_cpus_per_learner_worker": 0,
    "_fake_gpus": false,
    "local_gpu_idx": -1,
    "custom_resources_per_worker": {},
    "placement_strategy": "PACK"
  },
  "framework": "torch",
  "rollouts": {
    "env_runner_cls": null,
    "num_envs_per_worker": 1,
    "create_env_on_local_worker": false,
    "sample_async": false,
    "enable_connectors": true,
    "update_worker_filter_stats": true,
    "use_worker_filter_stats": true,
    "remote_worker_envs": false,
    "remote_env_batch_wait_ms": 0,
    "validate_workers_after_construction": true,
    "preprocessor_pref": "deepmind",
    "observation_filter": "NoFilter",
    "compress_observations": false,
    "enable_tf1_exec_eagerly": false,
    "sampler_perf_stats_ema_coef": null,
    "num_rollout_workers": 1,
    "duration_per_worker": 64,
    "duration_unit": "timesteps",
    "sample_timeout_s":180
  },
  "training": {
    "gamma": 0.99,
    "grad_clip": null,
    "grad_clip_by": "global_norm",
    "lr": 1e-3,
    "optimizer": {
      "type": "adam",
      "learning_rate": 0.002,
      "weight_decay": 0.0005 
    },
    "_enable_learner_api": false,
    
    "use_critic": true,
    "use_gae": true,
    "lambda_": 0.97,
    "use_kl_loss": true,
    "kl_coeff": 0.2,
    "kl_target": 0.01,
    "shuffle_sequences": true,
    "entropy_coeff": 0.01,
    "entropy_coeff_schedule": [
    [0, 0.002],  
    [70000, 0.0015],  
    [100000, 0.00128] 
    
    ],
    "v_min":0,
    "v_max":50,
    "vf_clip_param": 50,
    "vf_loss_coeff": 0.003,
    "vf_lr": 0.005,
    "normalize_advantages": true,
    "batch_size": 64,
    "num_trained_batches": 30,
    "model": {
      "custom_model": "lstm_model",
      "custom_model_config": {
        "seed":1234,
        "hidden_size":256,
        "use_lstm": true,  
        "max_seq_len": 10, 
        "lstm_cell_size": 256,
        "fun_layers": ["ReLU","ReLU"],
        "dropout": false,
        "dropout_list": [0.02,0.02],
        "n_features": [64,64]
      }
          }
  },
  "exploration": {
    "explore": true
  },
  "rl_module": {
    "_enable_rl_module_api": false
  },
  "evaluation":{
    "evaluation_duration_unit": "timesteps",
    "evaluation_duration_per_worker": 1088,
    "evaluation_config": {
      "explore": true,
      "entropy_coeff": 0.0002
    }
  },
  "callbacks":{
    "callbacks_class":"classes.Callbacks.MyCallbacks"
  }

}